{
  "name": "gitlab-ci-analytics",
  "version": "1.0.0",
  "description": "Web application to analyze GitLab CI build metrics",
  "main": "api/server.js",
  "scripts": {
    "start": "node api/server.js",
    "dev:backend": "nodemon api/server.js",
    "dev:frontend": "vite",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:vercel": "vercel dev --port 4000",
    "build": "vite build",
    "test": "jest",
    "test:backend": "jest tests/backend",
    "test:frontend": "jest tests/frontend"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "moment": "^2.29.4",
    "redis": "^4.6.7",
    "serverless-http": "^3.2.0",
    "uuid": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "chart.js": "^4.4.0",
    "react-bootstrap": "^2.9.1",
    "bootstrap": "^5.3.2",
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "@vitejs/plugin-react": "^4.2.0",
    "vite": "^5.0.0",
    "concurrently": "^8.2.0"

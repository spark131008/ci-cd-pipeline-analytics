{
  "version": 2,
  "functions": {
    "api/server.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    { 
      "src": "/api/(.*)", 
      "dest": "api/server.js" 
    },
    { 
      "src": "/(.*)", 
      "dest": "/dist/index.html" 
    }
  ],
  "devCommand": "npm run dev",
  "buildCommand": "npm run build"
}
